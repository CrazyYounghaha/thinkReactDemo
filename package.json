    "react-redux": "^5.0.3",
    "react-router": "3.x.x",
    "react-router-redux": "4.x.x",
    "redux": "3.x.x",
    "redux-logger": "2.x.x",
    "redux-thunk": "2.x.x"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-loader": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-es2015-loose": "8.x.x",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-1": "6.x.x",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-core": "6.x.x",
    "webpack": "2.x.x",
    "css-loader": "^0.26.2",
